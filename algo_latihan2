# Setting Variabel Nilai 
p = 10 
l = 20 
t = 50

# Perhitungan 
hasil =p*l*t
# Hasil Perhitungan
print("----------------------")
print("Masukkan nilai p:", p)
print("Masukkan nilai l:", l)
print("Masukkan nilai t:", t)
print("----------------------")
print("Hasil perhitungan: ", hasil)
print("----------------------")
